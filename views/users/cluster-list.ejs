<!-- ==========Header-Section========== -->
<header class="header-section">
    <div class="container">
        <div class="header-wrapper">
            <div class="logo">
                <a href="/user/">
                    <img
                        style="border-radius: 20px"
                        src="/user/images/logo/logo-cinema.png"
                        alt="logo"
                    />
                </a>
            </div>
            <ul class="menu">
                <li>
                    <a href="/user/">Trang chủ</a>
                </li>
                <li>
                    <a href="/user/movie-list">Phim</a>
                </li>
                <li>
                    <a href="/user/cluster-list" class="active">Cụm rạp</a>
                </li>
                <li>
                    <a href="/user/movie-ticket-plan">Lịch chiếu</a>
                </li>
                <li>
                    <a href="/user/contact">Liên hệ</a>
                </li>
                <li>
                    <a href="#">Thành viên</a>
                    <ul class="submenu">
                        <% if(currentUser != null && currentUser.user_role ==
                        true) { %>
                        <li>
                            <a
                                href="/user/movie-customer-profile"
                                class="active"
                                >Tài khoản</a
                            >
                        </li>
                        <li>
                            <a href="/user/ticket-list">Vé của tôi</a>
                        </li>
                        <li>
                            <a href="/admin">Quản lý</a>
                        </li>
                        <% } else { %>
                        <li>
                            <a
                                href="/user/movie-customer-profile"
                                class="active"
                                >Tài khoản</a
                            >
                        </li>
                        <li>
                            <a href="/user/ticket-list">Vé của tôi</a>
                        </li>

                        <% } %>
                    </ul>
                </li>
                <li class="header-button pr-0">
                    <% if(currentUser) { %>
                    <a><%=currentUser.user_name%></a>
                    <ul class="submenu">
                        <li>
                            <a href="/user/logout">Đăng xuất</a>
                        </li>
                    </ul>
                    <% } else { %>
                    <a href="/user/sign-in">Đăng nhập</a>
                    <% } %>
                </li>
            </ul>
            <div class="header-bar d-lg-none">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
</header>
<!-- ==========Header-Section========== -->

<!-- ==========Movie-Section========== -->
<section class="movie-section padding-top padding-bottom">
    <div class="container">
        <div class="tab">
            <div class="section-header-2">
                <div class="left">
                    <h2 class="title">Danh sách cụm rạp</h2>
                </div>
            </div>
            <div class="tab-area mb-30-none">
                <div class="tab-item active">
                    <div class="owl-carousel owl-theme tab-slider">
                        <div class="item">
                            <div class="movie-grid">
                                <div class="movie-thumb c-thumb">
                                    <a href="/user/cluster-detail">
                                        <img
                                            src="/user/images/movie/movie01.jpg"
                                            alt="movie"
                                        />
                                    </a>
                                </div>
                                <div class="movie-content bg-one">
                                    <h5 class="title m-0">
                                        <a href="#0">CGV Parson</a>
                                    </h5>
                                    <ul class="movie-rating-percent">
                                        <span class="content">188 Hung Vuong, Quan 5, TP.HCM</span>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="movie-grid">
                                <div class="movie-thumb c-thumb">
                                    <a href="#0">
                                        <img
                                            src="/user/images/movie/movie02.jpg"
                                            alt="movie"
                                        />
                                    </a>
                                </div>
                                <div class="movie-content bg-one">
                                    <h5 class="title m-0">
                                        <a href="#0">CGV Parson</a>
                                    </h5>
                                    <ul class="movie-rating-percent">
                                        <span class="content">188 Hung Vuong, Quan 5, TP.HCM</span>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ==========Movie-Section========== -->
